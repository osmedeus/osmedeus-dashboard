{
  "name": "osmedeus-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:nologin": "NEXT_PUBLIC_DISABLE_LOGIN=true next dev",
    "dev:nologin:turbo": "NEXT_PUBLIC_DISABLE_LOGIN=true next dev --turbopack",
    "dev:mock": "NEXT_PUBLIC_USE_MOCK=true NEXT_PUBLIC_DISABLE_LOGIN=true next dev",
    "dev:mock:turbo": "NEXT_PUBLIC_USE_MOCK=true NEXT_PUBLIC_DISABLE_LOGIN=true next dev --turbopack",
    "clean:build": "bun scripts/clean-build-artifacts.mjs",
    "build:base": "rm -rf build out && NEXT_EXPORT=true next build",
    "build": "bun run build:base && bun run clean:build",
    "build:static": "bun run build:base && bun run clean:build",
    "start": "next start",
    "start:nologin": "NEXT_PUBLIC_DISABLE_LOGIN=true next start",
    "lint": "eslint ."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@xyflow/react": "^12.4.4",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dagre": "^0.8.5",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.5.0",
    "sonner": "^1.7.3",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
